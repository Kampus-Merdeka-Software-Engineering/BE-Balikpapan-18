generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Resi {
  id          Int     @id @default(autoincrement())
  no_resi     String  @unique
  name        String?
  alamat      String?
  email       String?
  nama_barang String?
  origin      String?
  destination String?
  weight      String?
  courier     String?
}

model Contact {
  id    Int     @id @default(autoincrement())
  name  String?
  email String  @unique
}
